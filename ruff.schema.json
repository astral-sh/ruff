      "description": "The Python version to target, e.g., when considering automatic code upgrades.",
      "anyOf": [
        {
          "$ref": "#/definitions/PythonVersion"
        },
        {
          "type": "null"
        }
      ]
    },
    "unfixable": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/CheckCodePrefix"
