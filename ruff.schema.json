      "type": [
        "boolean",
        "null"
      ]
    },
    "select": {
      "description": "A list of check code prefixes to enable.",
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/CheckCodePrefix"
      }
